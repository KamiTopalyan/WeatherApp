<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FirstFragment">

    <ImageView
        android:id="@+id/day3Icon"
        android:layout_width="68dp"
        android:layout_height="71dp"
        android:layout_marginTop="-5dp"
        android:layout_marginEnd="10dp"
        android:src="@drawable/ic_launcher_foreground"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/day3Weather"
        app:srcCompat="@drawable/sun" />

    <ImageView
        android:id="@+id/day4Icon"
        android:layout_width="68dp"
        android:layout_height="71dp"
        android:layout_marginTop="-5dp"
        android:layout_marginEnd="10dp"
        android:src="@drawable/ic_launcher_foreground"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/day4Weather"
        app:srcCompat="@drawable/sun" />

    <TextView
        android:id="@+id/day1Weather"
        android:layout_width="213dp"
        android:layout_height="45dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/roboto"
        android:text="[PH] Weather"
        android:textAlignment="textEnd"
        android:textSize="34sp"
        app:layout_constraintEnd_toStartOf="@+id/day1Icon"
        app:layout_constraintTop_toBottomOf="@+id/divider" />

    <TextView
        android:id="@+id/day5Weather"
        android:layout_width="213dp"
        android:layout_height="45dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/roboto"
        android:text="[PH] Weather"
        android:textAlignment="textEnd"
        android:textSize="34sp"
        app:layout_constraintEnd_toStartOf="@+id/day5Icon"
        app:layout_constraintTop_toBottomOf="@+id/day4Weather" />

    <TextView
        android:id="@+id/day3Weather"
        android:layout_width="213dp"
        android:layout_height="45dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/roboto"
        android:text="[PH] Weather"
        android:textAlignment="textEnd"
        android:textSize="34sp"
        app:layout_constraintEnd_toStartOf="@+id/day3Icon"
        app:layout_constraintTop_toBottomOf="@+id/day2Weather" />

    <ImageView
        android:id="@+id/day1Icon"
        android:layout_width="68dp"
        android:layout_height="71dp"
        android:layout_marginTop="-5dp"
        android:layout_marginEnd="10dp"
        android:src="@drawable/ic_launcher_foreground"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/day1Weather"
        app:srcCompat="@drawable/sun" />

    <ImageView
        android:id="@+id/day2Icon"
        android:layout_width="68dp"
        android:layout_height="71dp"
        android:layout_marginTop="-5dp"
        android:layout_marginEnd="10dp"
        android:src="@drawable/ic_launcher_foreground"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/day2Weather"
        app:srcCompat="@drawable/sun" />

    <ImageView
        android:id="@+id/day5Icon"
        android:layout_width="68dp"
        android:layout_height="71dp"
        android:layout_marginTop="-5dp"
        android:layout_marginEnd="10dp"
        android:src="@drawable/ic_launcher_foreground"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/day5Weather"
        app:srcCompat="@drawable/sun" />

    <TextView
        android:id="@+id/day4Weather"
        android:layout_width="213dp"
        android:layout_height="45dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/roboto"
        android:text="[PH] Weather"
        android:textAlignment="textEnd"
        android:textSize="34sp"
        app:layout_constraintEnd_toStartOf="@+id/day4Icon"
        app:layout_constraintStart_toEndOf="@+id/day4"
        app:layout_constraintTop_toBottomOf="@+id/day3Weather" />

    <TextView
        android:id="@+id/day2Weather"
        android:layout_width="213dp"
        android:layout_height="45dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/roboto"
        android:text="[PH] Weather"
        android:textAlignment="textEnd"
        android:textSize="34sp"
        app:layout_constraintEnd_toStartOf="@+id/day2Icon"
        app:layout_constraintTop_toBottomOf="@+id/day1Weather" />

    <TextView
        android:id="@+id/Wind"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="[PH] Humi"
        app:layout_constraintEnd_toStartOf="@+id/HumiIcon"
        app:layout_constraintTop_toBottomOf="@+id/Temp" />

    <TextView
        android:id="@+id/MainWeather"
        android:layout_width="213dp"
        android:layout_height="45dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/roboto"
        android:text="[PH] Weather"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DayOfWeek" />

    <EditText
        android:id="@+id/DayOfWeek"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:fontFamily="@font/adamina"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="[PH] DayOfWeek" />

    <ImageView
        android:id="@+id/MainIcon"
        android:layout_width="68dp"
        android:layout_height="71dp"
        android:layout_marginStart="6dp"
        android:src="@drawable/ic_launcher_foreground"
        app:layout_constraintStart_toEndOf="@+id/MainWeather"
        app:layout_constraintTop_toBottomOf="@+id/DayOfWeek"
        app:srcCompat="@drawable/sun" />

    <ImageView
        android:id="@+id/TempIcon"
        android:layout_width="46dp"
        android:layout_height="53dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="9dp"
        android:src="@drawable/ic_launcher_foreground"
        app:layout_constraintBottom_toTopOf="@+id/HumiIcon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/temp" />

    <ImageView
        android:id="@+id/HumiIcon"
        android:layout_width="46dp"
        android:layout_height="53dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="9dp"
        android:src="@drawable/ic_launcher_foreground"
        app:layout_constraintBottom_toTopOf="@+id/WindIcon"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/water" />

    <ImageView
        android:id="@+id/WindIcon"
        android:layout_width="46dp"
        android:layout_height="53dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/ic_launcher_foreground"
        app:layout_constraintBottom_toTopOf="@+id/divider"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/wind" />

    <TextView
        android:id="@+id/Temp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        android:text="[PH] Temp"
        app:layout_constraintEnd_toStartOf="@+id/TempIcon"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="20dp"
        android:text="[PH] Wind"
        app:layout_constraintBottom_toTopOf="@+id/divider"
        app:layout_constraintEnd_toStartOf="@+id/WindIcon" />

    <View
        android:id="@+id/divider"
        android:layout_width="364dp"
        android:layout_height="5dp"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="22dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/WindIcon" />

    <TextView
        android:id="@+id/day1"
        android:layout_width="63dp"
        android:layout_height="46dp"
        android:layout_marginStart="24dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/day1Weather" />

    <TextView
        android:id="@+id/day2"
        android:layout_width="63dp"
        android:layout_height="46dp"
        android:layout_marginStart="24dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/day2Weather" />

    <TextView
        android:id="@+id/day3"
        android:layout_width="63dp"
        android:layout_height="46dp"
        android:layout_marginStart="24dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/day3Weather" />

    <TextView
        android:id="@+id/day4"
        android:layout_width="63dp"
        android:layout_height="46dp"
        android:layout_marginStart="24dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/day4Weather" />

    <TextView
        android:id="@+id/day5"
        android:layout_width="63dp"
        android:layout_height="46dp"
        android:layout_marginStart="24dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/day5Weather" />

</androidx.constraintlayout.widget.ConstraintLayout>